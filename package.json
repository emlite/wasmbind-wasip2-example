{
  "name": "wasmbind-example",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "cmake:config": "cmake -Bbin -GNinja -DCMAKE_BUILD_TYPE=Debug -DCMAKE_EXPORT_COMPILE_COMMANDS=ON -DCMAKE_TOOLCHAIN_FILE=~/wasi-sdk-25.0-x86_64-linux/share/cmake/wasi-sdk-p2.cmake",
    "cmake:build": "cmake --build bin",
    "prep": "cp -r node_modules/@bytecodealliance/preview2-shim dist/shim/preview2-shim",
    "jco:app": "npx jco transpile ./bin/main.wasm -o dist/app --instantiation",
    "serve": "npx http-server ./dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@bytecodealliance/jco": "^1.13.3",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "emlite": "^0.1.33"
  }
}
